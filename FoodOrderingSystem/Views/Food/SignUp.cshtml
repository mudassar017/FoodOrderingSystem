<script>
    $(document).ready(function () {
        $('#submit-button').click(function () {
            var Name = $('#txtName').val();
            var Number = $('#txtNumber').val();
            var Email = $('#txtEmail').val();
            var Password = $('#txtPassword').val();
            var Address = $('#txtAddress').val();

            var MyJson = { "name": Name, "number": Number, "email": Email, "password": Password, "address": Address, };



            $.ajax({
                type: "POST",
                url: "/api/Registration/RegistrationUser",
                data: JSON.stringify(MyJson),
                contentType: "application/json;",
                dataType: "json",
                success: function (response) {
                    if (response.status == "Insertion successfull") {
                        $('#txtName').val('');
                        $('#txtNumber').val('');
                        $('#txtEmail').val('');
                        $('#txtPassword').val('');
                        $('#txtAddress').val('');
                        $('#txtName').focus();
                        $('#lblMessage').html("Insertion successfull");
                        $('#lblMessage').css('color', 'green');
                    }
                    else {
                        $('#lblMessage').html("Insertion Failed");
                        $('#lblMessage').css('color', 'red');
                    }

                }















            });


        });
    });
</script>



